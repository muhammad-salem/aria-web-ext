{
   "background": {
      "scripts": [ "download.js" ]
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "Aria Download Manager",
   "homepage_url": "https://github.com/salemebo/aria",
   "icons": {
      "128": "aria.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "32",
   "name": "Aria Port",
   "offline_enabled": true,
   "permissions": [ "cookies", "webRequest", "webRequestBlocking", "*://*/*", "downloads", "nativeMessaging", "contextMenus", "activeTab" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1"
}
